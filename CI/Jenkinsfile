pipeline {
    agent { label "gcp-hodor-slave-generic"}
    options {
       // Add timestamps to output.
       timestamps()
       timeout(time: 45, unit: 'MINUTES')
    }
    environment {
        TEST = "test"
    }
    stages {
        stage('Build') {
            agent {
                docker {
                    image 'node:16.20.2'
                    reuseNode true
                }
            }
            steps {
                echo "${TEST}"
                sh 'yarn generate'
            }
        }
    }
}